PROGRAM myProgram
BEGIN
	
	FUNCTION INT myFunction (FLOAT a, INT b, FLOAT c, INT d)
	BEGIN
	  	FLOAT floatResult;
		INT intResult;
		floatResult := a+c;
		intResult := b+d;
		RETURN intResult;
	END

	
	FUNCTION INT main ()
	BEGIN
		FLOAT float1,float2;
		INT int1,int2,result;
		int1 := 4;
		int2 := 1;
		float1 := 1.0;
		float2 := 1.0;
		result := myFunction(float1, int1, float2, int2);
		WRITE(result);
		RETURN 0;
	END
END
